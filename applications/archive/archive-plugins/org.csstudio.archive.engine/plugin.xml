<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.csstudio.archive.engine.Application">
         </run>
      </application>
   </extension>
   <extension
         id="engine"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.csstudio.archive.engine.application"
            name="ArchiveEngine">
         <property
               name="appName"
               value="ArchiveEngine">
         </property>
      </product>
   </extension>
</plugin>
