importPackage(Packages.org.csstudio.sds.ui.scripting);

function execute() {
	/* Open a SDS display in a shell. */
	Functions.openDisplay("SDS Demo Display/Testcases/helper/composite1.css-sds");

	/* Wait for some seconds */
	Functions.wait(5);

	/* Close all instances of a SDS display. */
	Functions.closeDisplay("SDS Demo Display/Testcases/helper/composite1.css-sds");

}


